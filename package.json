{
  "name": "uiplay",
  "description": "UI wrapper for uxplay",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuminescentDev/uiplay.git"
  },
  "engines": {
    "node": ">=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.tauri": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "tauri": "tauri"
  },
  "devDependencies": {
    "@anuragroy/tailwindcss-animate": "^1.0.6",
    "@builder.io/qwik": "^1.18.0",
    "@builder.io/qwik-city": "^1.18.0",
    "@eslint/js": "latest",
    "@luminescent/ui": "^6.4.20",
    "@luminescent/ui-qwik": "^6.4.20",
    "@tailwindcss/vite": "^4.1.18",
    "@tauri-apps/api": "2.9.1",
    "@tauri-apps/cli": "2.9.6",
    "@tauri-apps/plugin-fs": "~2",
    "@types/node": "25.0.3",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-search": "^0.16.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "eslint": "9.39.2",
    "eslint-plugin-qwik": "^1.18.0",
    "globals": "16.5.0",
    "lucide-icons-qwik": "^0.540.0",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@10.26.2",
  "pnpm": {
    "overrides": {
      "stylus": "git+https://github.com/stylus/stylus.git"
    }
  }
}
